{
   "type":"object",
   "patternProperties":{
      "^([A-Z0-9]+([_-][A-Z0-9]+)*)$":{
         "type":"object",
         "properties":{
            "status":{
               "type":"string",
               "enum":[
                  "online",
                  "offline"
               ]
            },
            "status_message":{
               "type":"string"
            },
            "total_space":{
               "type":"integer"
            },
            "used_space":{
               "type":"integer"
            },
            "num_files":{
               "type":"integer"
            },
            "time_stamp":{
               "type":"integer"
            },
            "list_of_paths":{
               "type":"array",
               "items":{
                  "type":"string"
               }
            }
         }
      }
   },
   "additionalProperties":false
}